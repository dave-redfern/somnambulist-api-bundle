{
    "name": "somnambulist/api-bundle",
    "type": "library",
    "description": "An extension to add a few useful features to SamJs Fractal bundle and exception conversion",
    "keywords": [
        "api",
        "fractal",
        "json",
        "bundle",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "samj/fractal-bundle": "~2.0",
        "symfony/framework-bundle": "~4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "doctrine/collections": "~1.6",
        "pagerfanta/pagerfanta": "~2.0",
        "somnambulist/domain": "~2.0",
        "symfony/browser-kit": "~4.3",
        "symfony/phpunit-bridge": "~4.3",
        "symfony/stopwatch": "~4.3",
        "symfony/var-dumper": "~4.3",
        "symfony/yaml": "~4.3"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\ApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\ApiBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "suggest": {
        "pagerfanta/pagerfanta": "Pagination support expects Pagerfanta collections",
        "symfony/stopwatch": "Add profiling data from using fractal"
    }
}
